{
  "expo": {
    "name": "foodjuan",
    "slug": "foodjuan",
    "privacy": "public",
    "description": "A foodpanda clone app for juans made with react native and firebase.",
    "githubUrl": "https://github.com/kanamui/foodjuan",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FF2B85"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FF2B85"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
